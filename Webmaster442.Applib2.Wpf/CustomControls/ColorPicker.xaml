<UserControl
    x:Class="Webmaster442.Applib.Controls.ColorPicker"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:my="clr-namespace:Webmaster442.Applib.Controls"
    d:DesignHeight="160"
    d:DesignWidth="160"
    mc:Ignorable="d">
    <UserControl.Resources>
        <Style
            x:Key="Palette"
            TargetType="Rectangle">
            <Setter Property="Width" Value="15" />
            <Setter Property="Height" Value="15" />
            <Setter Property="Margin" Value="1" />
        </Style>
    </UserControl.Resources>
    <TabControl
        x:Name="Maintab">
        <TabItem
            Header="Picker">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition />
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition />
                    <ColumnDefinition Width="30" />
                    <ColumnDefinition Width="12" />
                </Grid.ColumnDefinitions>
                <Grid
                    Grid.Row="1"
                    Grid.Column="1"
                    Margin="5,0,5,0">
                    <Rectangle
                        x:Name="rectHueMonitor"
                        HorizontalAlignment="Stretch"
                        VerticalAlignment="Stretch">
                        <Rectangle.Fill>
                            <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
                                <GradientStop Offset="0.00" Color="#ffff0000" />
                                <GradientStop Offset="0.17" Color="#ffffff00" />
                                <GradientStop Offset="0.33" Color="#ff00ff00" />
                                <GradientStop Offset="0.50" Color="#ff00ffff" />
                                <GradientStop Offset="0.66" Color="#ff0000ff" />
                                <GradientStop Offset="0.83" Color="#ffff00ff" />
                                <GradientStop Offset="1.00" Color="#ffff0000" />
                            </LinearGradientBrush>
                        </Rectangle.Fill>
                    </Rectangle>
                    <Canvas
                        x:Name="HueSelector"
                        Height="8"
                        Margin="0,-4,0,0"
                        VerticalAlignment="Top">
                        <Path
                            Width="5"
                            Height="8"
                            Data="F1 M 276.761,316L 262.619,307.835L 262.619,324.165L 276.761,316 Z "
                            Fill="#FF000000"
                            Stretch="Fill"
                            Stroke="#FF000000"
                            StrokeLineJoin="Round" />
                        <Path
                            Canvas.Left="20"
                            Canvas.Top="8"
                            Width="5"
                            Height="8"
                            Data="F1 M 276.761,316L 262.619,307.835L 262.619,324.165L 276.761,316 Z "
                            Fill="#FF000000"
                            Stretch="Fill"
                            Stroke="#FF000000"
                            StrokeLineJoin="Round">
                            <Path.RenderTransform>
                                <RotateTransform Angle="180" />
                            </Path.RenderTransform>
                        </Path>
                    </Canvas>
                </Grid>

                <Grid
                    x:Name="colorArea"
                    Grid.Row="1"
                    Grid.Column="0">
                    <Rectangle
                        x:Name="rectSample"
                        Fill="Red" />
                    <Rectangle
                        x:Name="rectWhiteGradient">
                        <Rectangle.Fill>
                            <LinearGradientBrush StartPoint="0,0" EndPoint="1,0">
                                <GradientStop Offset="0" Color="#ffffffff" />
                                <GradientStop Offset="1" Color="#00ffffff" />
                            </LinearGradientBrush>
                        </Rectangle.Fill>
                    </Rectangle>
                    <Rectangle
                        x:Name="rectBlackGradient">
                        <Rectangle.Fill>
                            <LinearGradientBrush StartPoint="0,1" EndPoint="0, 0">
                                <GradientStop Offset="0" Color="#ff000000" />
                                <GradientStop Offset="1" Color="#00000000" />
                            </LinearGradientBrush>
                        </Rectangle.Fill>
                    </Rectangle>
                    <Canvas
                        x:Name="SampleSelector"
                        Width="8"
                        Height="8"
                        Margin="-2,-2,0,0"
                        HorizontalAlignment="Left"
                        VerticalAlignment="Top">
                        <Ellipse
                            Width="8"
                            Height="8"
                            Stroke="#FFFFFFFF"
                            StrokeThickness="2" />
                        <Ellipse
                            Width="8"
                            Height="8"
                            Stroke="#FF000000"
                            StrokeThickness="1" />
                    </Canvas>
                    <Rectangle
                        x:Name="rectSampleMonitor"
                        Fill="Transparent" />

                </Grid>

                <StackPanel
                    Grid.ColumnSpan="2"
                    Height="26"
                    Orientation="Horizontal">
                    <Grid
                        Margin="0,0,6,0">
                        <Image
                            x:Name="imgCheckerBoard"
                            Width="20"
                            Height="20"
                            Stretch="None" />
                        <Rectangle
                            x:Name="SelectedColor"
                            Width="20"
                            Height="20"
                            Fill="White"
                            Stroke="Black" />
                    </Grid>
                    <TextBox
                        x:Name="HexValue"
                        Canvas.Left="5"
                        Canvas.Top="4"
                        Width="70"
                        Height="22"
                        VerticalAlignment="Center"
                        Background="#4fFFFFFF"
                        BorderBrush="#4F444444"
                        FontSize="10"
                        Foreground="Black"
                        Text="#FFFFFFFF"
                        TextChanged="HexValue_TextChanged" />
                </StackPanel>
                <my:AlphaSelectControl
                    x:Name="ctlAlphaSelect"
                    Grid.Row="1"
                    Grid.Column="2"
                    Width="8"
                    Height="95"
                    Margin="0,0,2,0"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top" />
            </Grid>
